{
  "name": "@astriel-division/ts-config",
  "version": "1.0.2",
  "scripts": {
    "postinstall": "husky install .github/husky",
    "commit": "git-cz"
  },
  "dependencies": {
    "tslib": "2.3.1",
    "typescript": "4.5.2"
  },
  "devDependencies": {
    "@types/node": "16.7.8",
    "cz-conventional-changelog": "3.3.0",
    "rimraf": "3.0.2",
    "standard-version": "9.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AstrielDivision/ts.git"
  },
  "bugs": {
    "url": "https://github.com/AstrielDivision/ts/issues"
  },
  "files": [
    "src/**"
  ],
  "keywords": [
    "tsconfig",
    "config"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
